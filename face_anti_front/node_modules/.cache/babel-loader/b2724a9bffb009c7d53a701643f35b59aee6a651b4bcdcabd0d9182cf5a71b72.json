Track.value = stream.getTracks()[0];\r\n        }).catch((err)=>{\r\n            console.log(err)\r\n        })\r\n    }else{\r\n        //防止浏览器版本旧不支持mediaDevices\r\n        navigator.mediaDevices={}\r\n    }\r\n}\r\nonBeforeUnmount(()=>{\r\n    closeCamera();\r\n})\r\nfunction closeCamera(){\r\n    let stream = video.value.srcObject;\r\n    if (!stream) return;\r\n    let tracks = stream.getTracks();\r\n    tracks.forEach((x) => {\r\n    x.stop();\r\n    });\r\n}\r\nasync function getDetection(){\r\n    //把之前摄像头捕获的视频流发送给后端\r\n\r\n    \r\n    //接收后端返回的检测结果\r\n    \r\n}\r\nfunction closeDetection(){\r\n    // socket.onclose = ()=>{\r\n    //     console.log(\"close\");\r\n    // }\r\n    socket.send(\"bbbbb\")\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.container{\r\n    display: flex;\r\n    width: 84vw;\r\n    max-height: 90vh;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    background-color: #f0f0f0;\r\n    padding-top: 1%;\r\n    padding-bottom: 3%;\r\n}\r\n#camera{\r\n    width:0;\r\n    height:0;\r\n    display: none;\r\n}\r\n#video{\r\n    width:60vw;\r\n}\r\n.botton_box{\r\n    margin-top: 10px;\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n</style>"],"mappings":"AAiBA,SAAQA,GAAG,EAACC,SAAS,EAACC,eAAe,QAAQ,KAAK;;;;;;;IAClD,MAAMC,KAAK,GAAGH,GAAG,CAAC,CAAC;IACnB,MAAMI,gBAAgB,GAAGJ,GAAG,CAAC,CAAC,CAAC,CAAC;IAChC,MAAMK,MAAM,GAAGL,GAAG,CAAC,CAAC;IACpB,MAAMM,GAAG,GAAGN,GAAG,CAAC,CAAC;IACjB,MAAMO,MAAM,GAAG,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IACnDP,SAAS,CAAC,MAAI;MACVQ,OAAO,CAACC,GAAG,CAAC,YAAY,CAAC;MACzBC,SAAS,CAAC,CAAC;MACX;MACAJ,MAAM,CAACK,MAAM,GAAG,MAAI;QAChBH,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;MAChC,CAAC;IACL,CAAC,CAAC;IACF,SAASC,SAASA,CAAA,EAAE;MAChBN,MAAM,CAACQ,KAAK,GAAGC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;MAChDT,GAAG,CAACO,KAAK,GAAGR,MAAM,CAACQ,KAAK,CAACG,UAAU,CAAC,IAAI,CAAC;MACzC,IAAGC,SAAS,CAACC,YAAY,EAAC;QACtBD,SAAS,CAACC,YAAY,CAACC,YAAY,CAAC;UAAChB,KAAK,EAAC,IAAI;UAACiB,KAAK,EAAC;QAAK,CAAC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAG;UACzE