ary\" @click=\"function(){}\">保存截图</el-button>\r\n        \r\n    </div>\r\n    \r\n </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { Sleep } from '@icon-park/vue-next';\r\nimport {ref,onMounted,onBeforeUnmount,} from 'vue';\r\nconst video = ref()\r\nconst mediaStreamTrack = ref({})\r\nconst canvas = ref()\r\nconst ctx = ref()\r\nconst socket = new WebSocket('ws://localhost:8888');\r\nlet flag = 1;\r\nonMounted(()=>{\r\n    console.log('正在打开摄像头。。。')\r\n    getCamera();\r\n    //连接后端socket\r\n    socket.onopen = ()=>{\r\n        console.log('socket 已打开...')\r\n    }\r\n})\r\nfunction getCamera(){\r\n    canvas.value = document.getElementById('camera');\r\n    ctx.value = canvas.value.getContext('2d');\r\n    if(navigator.mediaDevices){\r\n        navigator.mediaDevices.getUserMedia({video:true,audio:false}).then((stream)=>{\r\n            video.value.srcObject = stream;\r\n            video.value.play();\r\n            mediaStreamTrack.value = stream.getTracks()[0];\r\n        }).catch((err)=>{\r\n            console.log(err)\r\n        })\r\n    }else{\r\n        //防止浏览器版本旧不支持mediaDevices\r\n        navigator.mediaDevices={}\r\n    }\r\n}\r\nonBeforeUnmount(()=>{\r\n    closeCamera();\r\n})\r\nfunction closeCamera(){\r\n    let stream = video.value.srcObject;\r\n    if (!stream) return;\r\n    let tracks = stream.getTracks();\r\n    tracks.forEach((x) => {\r\n    x.stop();\r\n    });\r\n}\r\nfunction getDetection(){\r\n    flag = 1;  \r\n    console.log(\"bbbbb\")\r\n    sendStream();\r\n}\r\nfunction sendStream(){\r\n    console.log(\"aaaaa\")\r\n    const canvas = document.createElement('canvas');\r\n    canvas.width = video.value.videoWidth;\r\n    canvas.height = video.value.videoHeight;\r\n    console.log(video.value.videoWidth)\r\n    const ctx=canvas.getContext('2d');\r\n    ctx.drawImage(video.value, 0, 0, canvas.width, canvas.height);\r\n    const imageData = ctx.getImageData(0, 0, canvas.width, canvas.height);\r\n    // 将图像数据发送给后端处理\r\n    socket.onmessage=()=>{\r\n        socket.send(\"bbbbbbbb\");  \r\n    }\r\n    \r\n    \r\n}\r\nfunction closeDetection(){\r\n    flag = 0;\r\n    socket.send(\"bbbbbbbb\");   \r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.container{\r\n    display: flex;\r\n    width: 84vw;\r\n    max-height: 90vh;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    background-color: #f0f0f0;\r\n    padding-top: 1%;\r\n    padding-bottom: 3%;\r\n}\r\n#camera{\r\n    width:0;\r\n    height:0;\r\n    display: none;\r\n}\r\n#video{\r\n    width:60vw;\r\n}\r\n.botton_box{\r\n    margin-top: 10px;\r\n    display: flex;\r\n    flex-direction: row;\r\n}\r\n</style>"],"mappings":"AAiBA,SAASA,KAAK,QAAQ,qBAAqB;AAC3C,SAAQC,GAAG,EAACC,SAAS,EAACC,eAAe,QAAQ,KAAK;;;;;;;IAClD,MAAMC,KAAK,GAAGH,GAAG,CAAC,CAAC;IACnB,MAAMI,gBAAgB,GAAGJ,GAAG,CAAC,CAAC,CAAC,CAAC;IAChC,MAAMK,MAAM,GAAGL,GAAG,CAAC,CAAC;IACpB,MAAMM,GAAG,GAAGN,GAAG,CAAC,CAAC;IACjB,MAAMO,MAAM,GAAG,IAAIC,SAAS,CAAC,qBAAqB,CAAC;IACnD,IAAIC,IAAI,GAAG,CAAC;IACZR,SAAS,CAAC,MAAI;MACVS,OAAO,CAACC,GAAG,CAAC,YAAY,CAAC;MACzBC,SAAS,CAAC,CAAC;MACX;MACAL,MAAM,CAACM,MAAM,GAAG,MAAI;QAChBH,OAAO,CAACC,GAAG,CAAC,eAAe,CAAC;MAChC,CAAC;IACL,CAAC,CAAC;IACF,SAASC,SAASA,CAAA,EAAE;MAChBP,MAAM,CAACS,KAAK,GAAGC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;MAChDV,GAAG,CAACQ,KAAK,GAAGT,MAAM,CAACS,KAAK,CAACG,UAAU,CAAC,IAAI,CAAC;MACzC,IAAGC,SAAS,CAACC,YAAY,EAAC;QACtBD,SAAS,CAACC,YAAY,CAACC,YAAY,CAAC;UAACjB,KAAK,EAAC,IAAI;UAACkB,KAAK,EAAC;QAAK,CAAC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAG;UACzEpB,KAAK,CAACW,KAAK,CAACU,SAAS,GAAGD,MAAM;UAC9BpB,KAAK,CAACW,KAAK,CAACW,IAAI,CAAC,CAAC;UAClBrB,gBAAgB,CAACU,KAAK,GAAGS,MAAM,CAACG,SAAS,CAAC,CAAC,CAAC,CAAC,CAAC;QAClD,CAAC,CAAC,CAACC,KAAK,CAAEC,GAAG,IAAG;UACZlB,OAAO,CAACC,GAAG,CAACiB,GAAG,CAAC;QACpB,CAAC,CAAC;MACN,CAAC,MAAI;QACD;QACAV,SAAS,CAACC,YAAY,GAAC,CAAC,CAAC;MAC7B;IACJ;IACAjB,eAAe,CAAC,MAAI;MAChB2B,WAAW,CAAC,CAAC;IACjB,CAAC,CAAC;IACF,SAASA,WAAWA,CAAA,EAAE;MAClB,IAAIN,MAAM,GAAGpB,KAAK,CAACW,KAAK,CAACU,SAAS;MAClC,IAAI,CAACD,MAAM,EAAE;MACb